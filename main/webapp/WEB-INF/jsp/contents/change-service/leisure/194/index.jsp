						<script type="text/javascript">
							jQuery(document).ready(function(){
								jQuery(".chngSrvDiv .cont3 li").last().css('backgroundImage', 'none');

								/* 탭버튼 */
								var index = 0;
								jQuery(".tab1 a").each(function(e){
									jQuery(this).click(function(){
										if(index != e){
											jQuery(".tab1 a").eq(index).removeClass("on");
											jQuery(".tabDiv").hide();
											index = e;
											jQuery(".tab1 a").eq(index).addClass("on");
											jQuery(".tabDiv").eq(index).show();
										}
									});
								});

								var curService2 = 0;
								jQuery(".chngSrvDiv .subSelDiv").each(function(q){
									jQuery(this).find(".selUnit").css('display', 'none');
									jQuery(this).find(".selUnit").eq(0).css('display', 'block');
									jQuery(this).find(".subSel").each(function(k){
										jQuery(this).css('left', 90*k);
										jQuery(this).click(function(){
											if(curService2 != k){
												jQuery(this).parent(".subSelDiv").find(".subSel").eq(curService2).removeClass("on");
												jQuery(this).parent(".subSelDiv").find(".selUnit").eq(curService2).css('display', 'none');
												curService2 = k;
												jQuery(this).parent(".subSelDiv").find(".subSel").eq(curService2).addClass("on");
												jQuery(this).parent(".subSelDiv").find(".selUnit").eq(curService2).css('display', 'block');
											}
										});
									});
								});
							});
						</script>

