<script>
	jQuery(document).ready(function(){
		/* 탭버튼 */
		var index = 0;
		jQuery(".tab1 a").each(function(e){
			jQuery(this).click(function(){
				if(index != e){
					jQuery(".tab1 a").eq(index).removeClass("on");
					jQuery(".tabDiv").hide();
					index = e;
					jQuery(".tab1 a").eq(index).addClass("on");
					jQuery(".tabDiv").eq(index).show();
				}
			})
		})
		
		/* 2017-04-19 수정 */
		//tabBox jQuery삭제
		jQuery(".moveBtn").click(function(){
			var move = jQuery("#benefit").offset().top;
			$('html, body').animate({scrollTop : move}, 700);
		});
		/* //2017-04-19 수정 */


		/* 전체열기 버튼 */
		jQuery("a.allOpen").click(function(){
			jQuery(".accoBoard li a").addClass("on");
			jQuery(".accoBoard li div.accoBox").slideDown(300);
		})

		/*전체닫기 버튼*/
		jQuery("a.allClose").click(function(){
			jQuery(".accoBoard li a").removeClass("on");
			jQuery(".accoBoard li div.accoBox").slideUp(300);
		})

		/* 아코디언 */
		jQuery(".accoBoard li a").click(function(){
			if(jQuery(this).hasClass("on")){
				jQuery(this).removeClass("on");
				jQuery(this).siblings(".accoBox").slideUp(300);
			}else{
				jQuery(".accoBoard li a").removeClass("on");
				jQuery(".accoBoard li a").siblings(".accoBox").slideUp(300);
				jQuery(this).addClass("on");
				jQuery(this).siblings(".accoBox").slideDown(300);
			}
		})

		/* 제품정보 마진조절 */
		jQuery(".product .pdDiv").each(function(num){
			jQuery(".product .pdDiv").eq((num*3)+2).css('marginRight', 0);
		})

		/* 제품정보 마진조절 */
		jQuery(".product3 .pdDiv").each(function(num){
			jQuery(".product3 .pdDiv").eq((num*3)+2).css('marginRight', 0);
		})
	})
</script>

