<script type="text/javascript">
	window.onload = function() {
        var $agrees = document.getElementsByClassName('agree');
        for (var i = 0; i < $agrees.length; i++) {
            $agrees[i].checked = false;
        }
	}

	function onClickCancel() {
	    var confirmFlag = confirm('취소 시 기존 아이디로 대명 아임레디 사이트에 로그인 하실 수 없습니다. 그래도 취소 하시겠습니까?');
	    if (confirmFlag) {
            location.replace("/main/index.do");
		}
	}

	function checkAgreements() {
        var $agrees = document.getElementsByClassName('agree');
        for (var i = 0; i < $agrees.length; i++) {
            if ($agrees[i].checked == false) {
                alert("이용약관 및 개인정보 수집 및 이용에 대해 동의하여주세요.");
                return false;
			}
		}
		document.integrationIdsFrm.submit();
	}
</script>