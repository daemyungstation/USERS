						<script type="text/javascript">
						//<![CDATA[
						           
							jQuery(document).ready(function(){
								//tab
								var index = 0;
								jQuery(".tab1 a").each(function(e){
									jQuery(this).click(function(){
										if(index != e){
											jQuery(".tab1 a").eq(index).removeClass("on");
											jQuery(".tabDiv").hide();
											index = e;
											jQuery(".tab1 a").eq(index).addClass("on");
											jQuery(".tabDiv").eq(index).show();
										}
									})
								});
								
								getDtlPageList();
								
								getFaqPageList();
							});
							
							//dtl 리스트 가져오기
							function getDtlPageList()
							{
								var pageIndex = "";
								
								if(arguments.length > 0)
								{
									pageIndex = arguments[0];
								}
								else
								{
									pageIndex = 1;
								}
								
								jQuery.post("/change-service/dtl-list.ajax",
									{
										"pageIndex" : pageIndex,
										"f" : jQuery("select[name='f']").val(),
										"q" : jQuery("input[name='q']").val(),
										"prdctCd" : "PRDCT04"
									},
									function(text) 
									{
										jQuery("#dtlDiv").html("");
										jQuery("#dtlDiv").html(text);
									},
									"text"
								).fail(function () {
									alert("예기치 않은 오류입니다.");
								});
							}
							
							//faq 리스트 가져오기
							function getFaqPageList()
							{	
								var pageIndex = "";
								
								if(arguments.length > 0)
								{
									if(arguments[0] == "all")
									{
										pageIndex = 1;
										jQuery("select[name='f']").val("");
										jQuery("input[name='q']").val("");
									}
									else
									{
										pageIndex = arguments[0];
									}
								}
								else
								{
									pageIndex = 1;
								}
								
								jQuery.post("/change-service/faq-list.ajax",
									{
										"pageIndex" : pageIndex,
										"f" : jQuery("select[name='f']").val(),
										"q" : jQuery("input[name='q']").val(),
										"srvcId" : 250
									},
									function(text) 
									{
										jQuery("#faqDiv").html("");
										jQuery("#faqDiv").html(text);
									},
									"text"
								).fail(function () {
									alert("예기치 않은 오류입니다.");
								});
							}

						//]]>
						</script>